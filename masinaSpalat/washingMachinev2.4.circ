-- Date    :
--
--------------------------------------------------------------------------------
-- Description :
--
--------------------------------------------------------------------------------

LIBRARY ieee;
USE ieee.std_logic_1164.all;
USE ieee.std_logic_unsigned.all;


ENTITY DivizorFrecv IS
					
  PORT (
  --Insert input ports below
 					-- numar ciclu				 0   1   2   3   4   5          49                 98   99  0   1   2
    clock      : IN  std_logic;                    -- tactul la 100 Mhz de ex. _|`\_|`\_|`\_|`\_|`\_|`\........_|`\_.............._|`\_|`\_|`\_|`\_|`\
    div_clk    : out std_logic				-- ceas divizat cu 100        __________________________________|``````````````````````\___________
    );
END DivizorFrecv;

--sa fie un numarator, si sa se afiseze pe 7 segment valorile/iesirile =&gt; va numara la frecventa clockului din placuta 100Hz =&gt; 
--------------------------------------------------------------------------------
--divizor de frecventa de la 100 Mhz la 1 Mhz de ex.

ARCHITECTURE TypeArchitecture OF DivizorFrecv IS

signal tmp:std_logic_vector (6 downto 0):=(others =&gt;'0');--"0000000"
BEGIN

process (clock)
begin
	if (rising_edge(clock)) then
		if tmp = ("1100011") then tmp &lt;="0000000";     -- verificam daca am ajuns la al 99'lea ciclu
		elsif (tmp &lt;= "0110001") then 		-- -- verificam daca am ajuns la al 49'lea ciclu
			tmp &lt;= tmp + 1;  -- timp de 50 de cicluri clock-ul divizat e '0'
			div_clk &lt;= '0';
		else  div_clk &lt;= '1';				-- timp de 50 de cicluri clock-ul divizat e '1'
			tmp &lt;= tmp + 1;
	end if;
	end if;
end process;

END TypeArchitecture;
</vhdl>
  <vhdl name="Timer60">LIBRARY ieee;
USE ieee.std_logic_1164.all;
USE ieee.std_logic_unsigned.all;
USE ieee.numeric_std.all;

ENTITY Timer60 IS
  PORT (
  	clk: in std_logic;
  	enable: in std_logic;
  	
  	TC: out std_logic
    );
END Timer60;

ARCHITECTURE TypeArchitecture OF Timer60 IS

BEGIN
process(clk, enable)

variable count: integer :=0;

begin

if(rising_edge(clk)) then
	if(enable = '1') then
		if(count = 60) then
			count :=0;
			TC &lt;= '1';
		else
			count := count+1;
			TC &lt;= '0';
		end if;
	else
		count := 0;
		TC &lt;= '0';
	end if;
end if;
	
end process;

END TypeArchitecture;


