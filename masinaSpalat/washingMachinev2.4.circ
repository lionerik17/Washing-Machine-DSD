  <vhdl name="UnitateCentrala">LIBRARY ieee;
USE ieee.std_logic_1164.all;
USE ieee.std_logic_unsigned.all;
USE ieee.numeric_std.all;

ENTITY UnitateCentrala IS
  PORT (
  	-- intrari ale UC
  	clock, start, usa, anulare: in std_logic;
  	temperatura: in std_logic_vector(1 downto 0);
  	viteza: in std_logic_vector(1 downto 0);
  	prespalare: in std_logic;
  	clatireSuplimentara: in std_logic;
  	mode: in std_logic_vector (2 downto 0);
	modSelectie: in std_logic;
	-- iesiri de la UC pentru UE
 	enPrespalare, enSpalare, enClatire, enClatireSuplimentara, enCentrifuga: out std_logic;
	-- iesiri de la UE pentru UC
 	gtPrespalare, gtSpalare, gtClatire, gtClatireSuplimentara, gtCentrifuga: in std_logic;

 	--iesire UC
 	gataSpalare: out std_logic
    );
END UnitateCentrala;

ARCHITECTURE turaDeSpalat OF UnitateCentrala IS

type State_T is (A_Idle, B_Prespalare, B_Spalare, C_Clatire, C_ClatireSuplimentara, D_Centrifuga, E_Gata);
signal State, nextState: State_T;
signal vitezaSelectata, rot: integer :=0;
signal spalare2, clatire2: integer :=0;
signal gata: integer :=0;

-- A -&gt; idle, setari predefinite, setari manuale
-- B -&gt; spalare/prespalare
-- C -&gt; clatire
-- D -&gt; centrifuga
-- E -&gt; gata

-- moduri de spalare
-- 000 - spalare rapida
-- 001 - camasi
-- 010 - culori inchise
-- 011 - rufe murdare
-- 100 - antialergic

-- temperatura
-- 00 - 30
-- 01 - 40
-- 10 - 60
-- 11 - 90

-- modSelectie
-- 0, manual
-- 1, automat

BEGIN

-- dam update la stare
Update: process(clock, anulare)
		begin
			if(anulare = '1') then
				State &lt;= A_Idle;
			elsif (rising_edge(clock)) then
				State &lt;= NextState;
			end if;
		end process Update;

-- vezi organigrama
Transitions: process(start, anulare, usa, viteza, mode, modSelectie, prespalare, spalare2, clatireSuplimentara, clatire2, State, gtPrespalare, gtSpalare, gtClatire, gtClatireSuplimentara, gtCentrifuga)
			begin
				if(anulare = '1') then nextState &lt;= A_Idle;
				else
				case State is
					when A_Idle =&gt;
					if(start = '1' and usa= '1' and (prespalare = '1' or spalare2 = 1)) then nextState &lt;= B_Prespalare;
					elsif (start ='1' and usa = '1' and (prespalare = '0' or spalare2 = 0)) then nextState &lt;= B_Spalare;
					else nextState &lt;= A_Idle;
					end if;

					when B_Prespalare =&gt;
					if(gtPrespalare = '1') then nextState &lt;= B_Spalare;
					else nextState &lt;= B_Prespalare;
					end if;

					when B_Spalare =&gt;
					if(gtSpalare = '1') then nextState &lt;= C_Clatire;
					else nextState &lt;= B_Spalare;
					end if;
					
					when C_Clatire =&gt;
					if(gtClatire = '1' and (clatireSuplimentara = '1' or clatire2 = 1)) then nextState &lt;= C_ClatireSuplimentara;
					elsif(gtClatire = '1' and (clatireSuplimentara = '0' or clatire2 = 0)) then nextState &lt;= D_Centrifuga;
					else nextState&lt;=C_Clatire;
					end if;

					when C_ClatireSuplimentara =&gt;
					if(gtClatireSuplimentara = '1') then nextState &lt;= D_Centrifuga;
					else nextState &lt;= C_ClatireSuplimentara;
					end if;
					
					when D_Centrifuga =&gt;
					if(gtCentrifuga = '1') then nextState &lt;= E_Gata;
					else nextState &lt;= D_Centrifuga;
					end if;

					when E_Gata =&gt;
					nextState &lt;= A_Idle;
				end case;
				end if;
			end process Transitions;

-- vezi relatii UC-UE
Outputs: process(start, anulare, viteza, usa, mode, modSelectie, prespalare, spalare2, clatireSuplimentara, clatire2, State, gtPrespalare, gtSpalare, gtClatire, gtClatireSuplimentara, gtCentrifuga)
		begin
			case State is
				when A_Idle =&gt;
				vitezaSelectata &lt;= 0; rot &lt;= 0;
				enPrespalare&lt;='0'; enSpalare&lt;='0'; enClatire &lt;='0'; enClatireSuplimentara&lt;='0'; enCentrifuga&lt;='0';
				if(gata = 0 or anulare = '1') then 
					gataSpalare &lt;= '0'; gata &lt;= 0;
					spalare2 &lt;= 0; clatire2 &lt;= 0;
				end if;
				
				if(modSelectie = '1') then
					if(to_integer(unsigned(mode)) = 0) then vitezaSelectata &lt;= 1200;
					elsif(to_integer(unsigned(mode)) = 1) then vitezaSelectata &lt;= 800;
					elsif(to_integer(unsigned(mode)) = 2) then vitezaSelectata &lt;= 1000; clatire2 &lt;= 1;
					elsif(to_integer(unsigned(mode)) = 3) then vitezaSelectata &lt;= 1000; spalare2 &lt;= 1;
					elsif(to_integer(unsigned(mode)) = 4) then vitezaSelectata &lt;= 1200; clatire2 &lt;= 1;
					end if;
				else
					if(viteza = 0) then vitezaSelectata &lt;= 800;
					elsif(viteza = 1) then vitezaSelectata &lt;= 1000;
					elsif(viteza = 2) then vitezaSelectata &lt;= 1200;
					end if;
				end if;
	
				when B_Prespalare =&gt;
				if(prespalare = '1' or spalare2 = 1) then
					enPrespalare &lt;= '1'; rot &lt;= 60;
				end if;

				when B_Spalare =&gt;
				if(gata = 0) then enSpalare&lt;='1'; rot &lt;= 60; end if;
				
				when C_Clatire =&gt;
				if(gata = 0) then enClatire &lt;= '1'; rot &lt;= 120; end if; -- enClatire

				when C_ClatireSuplimentara =&gt;
				if(clatireSuplimentara = '1' or clatire2 = 1) then 
					if(gata = 0) then enClatireSuplimentara &lt;= '1'; rot &lt;= 120; end if; -- enClatireSuplimentara
				end if;
	
				when D_Centrifuga =&gt;
				if(gata = 0) then enCentrifuga &lt;= '1'; rot &lt;= vitezaSelectata; end if;
			
				when E_Gata =&gt;
				if(gata = 0) then gataSpalare &lt;= '1'; gata &lt;= 1; rot &lt;= 0; end if;
			end case;
		end process Outputs;

END turaDeSpalat;</vhdl>
  <vhdl name="UnitateExecutie">LIBRARY ieee;
USE ieee.std_logic_1164.all;
USE ieee.std_logic_unsigned.all;
USE ieee.numeric_std.all;

ENTITY UnitateExecutie IS
  PORT (
  	clk: in std_logic;
  	time: in std_logic_vector(8 downto 0);
  	timeSpalare, timePrespalare: in std_logic_vector(8 downto 0);
  	start, usa, anulare: in std_logic;
  	enPrespalare, enSpalare, enClatire, enClatireSuplimentara, enCentrifuga, gataSpalare, gataUsa: in std_logic;
  	temperatura, viteza: in std_logic_vector(1 downto 0);
  	prespalare, clatireSuplimentara: in std_logic;
  	mode: in std_logic_vector(2 downto 0);
  	modSelectie: in std_logic;
  	
  	
  	elapsedTime: out std_logic_vector(8 downto 0);
  	gtPrespalare, gtSpalare, gtClatire, gtClatireSuplimentara, gtCentrifuga, enT60: out std_logic
    );
END UnitateExecutie;

-- instead of using 11 timers like in v1.0, I implemented one single timer that is similar to the CU
-- 5 variables to count all the washing stages
-- formula for time determined in DecoderTime
-- formula for spalare and prespalare determined in DecoderTemp

ARCHITECTURE TypeArchitecture OF UnitateExecutie IS

signal tempSel, vitSel, modeSel: integer:=0;
signal prespSel, clatSel, modSelSel: std_logic;
BEGIN

process(tempSel, vitSel, modeSel, modSelSel, prespSel, clatSel, clk, time, timeSpalare, timePrespalare, start, usa, anulare, enPrespalare, enSpalare, enClatire, enClatireSuplimentara, enCentrifuga, gataSpalare, gataUsa, prespalare, clatireSuplimentara, modSelectie, mode)

variable count: integer :=0;
variable stop: integer :=0;
variable started: integer :=0;
variable countPrespalare, countSpalare, countClatire, countClatireSuplimentara, countCentrifuga: integer :=0;

begin
if(rising_edge(clk)) then
	tempSel &lt;= to_integer(unsigned(temperatura));
	vitSel &lt;= to_integer(unsigned(viteza));
	modeSel &lt;= to_integer(unsigned(mode));
	prespSel &lt;= prespalare;
	clatSel &lt;= clatireSuplimentara;
	modSelSel &lt;= modSelectie;

	if(vitSel = 3) then
		count:=-1; stop:=0;
		countPrespalare:=0; countSpalare:=0; countClatire:=0; countClatireSuplimentara:=0; countCentrifuga:=0;
		gtPrespalare&lt;='0'; gtSpalare&lt;='0'; gtClatire&lt;='0'; gtClatireSuplimentara&lt;='0'; gtCentrifuga&lt;='0'; enT60 &lt;= '0';
	end if;
	
	if(anulare = '1' or gataSpalare = '1' or start = '0' or usa = '0') then
		count:=0; stop:=0;
		countPrespalare:=0; countSpalare:=0; countClatire:=0; countClatireSuplimentara:=0; countCentrifuga:=0;
		gtPrespalare&lt;='0'; gtSpalare&lt;='0'; gtClatire&lt;='0'; gtClatireSuplimentara&lt;='0'; gtCentrifuga&lt;='0'; enT60 &lt;= '0';
	end if;
	if(start = '1' and usa = '1') then
		if(count &gt; 0) then
			if(tempSel /= temperatura or vitSel /= viteza or modeSel /= mode or
			   prespSel /= prespalare or clatSel /= clatireSuplimentara or modSelSel /= modSelectie) then
			   count:=-2; stop:=0;
			   countPrespalare:=0; countSpalare:=0; countClatire:=0; countClatireSuplimentara:=0; countCentrifuga:=0;
		        gtPrespalare&lt;='0'; gtSpalare&lt;='0'; gtClatire&lt;='0'; gtClatireSuplimentara&lt;='0'; gtCentrifuga&lt;='0'; enT60 &lt;= '0';
			end if;
		end if;
		if(count = to_integer(unsigned(time)) and stop = 0) then
			enT60 &lt;= '1';
			if(gataUsa = '1') then gtCentrifuga &lt;= '1'; stop :=1;
			end if;
		elsif(stop = 0) then
			if(enPrespalare = '1') then 
				if(countPrespalare = timePrespalare) then gtPrespalare &lt;= '1';
				elsif(gataSpalare = '0' and anulare = '0') then countPrespalare := countPrespalare + 1; count := count + 1;
				end if;
			end if;
			if(enSpalare = '1') then 
				if(countSpalare = timeSpalare) then gtSpalare &lt;= '1'; 
				elsif(gataSpalare = '0' and anulare = '0') then  countSpalare := countSpalare + 1; count := count + 1;
				end if; 
			end if;
			if(enClatire = '1') then 
				if(countClatire = 10) then gtClatire &lt;='1';
				elsif(gataSpalare = '0' and anulare = '0') then  countClatire := countClatire + 1; count := count + 1;
				end if; 
			end if;
			if(enClatireSuplimentara = '1') then
				if(countClatireSuplimentara = 10) then gtClatireSuplimentara &lt;= '1';
				elsif(gataSpalare = '0' and anulare = '0') then  countClatireSuplimentara := countClatireSuplimentara + 1; count := count + 1;
				end if;
			end if;
			if(enCentrifuga = '1') then
				if(gataSpalare = '0' and anulare = '0') then  countCentrifuga := countCentrifuga + 1; count := count + 1;
				end if;
			end if; 
		end if;
	end if;
end if;
	if(stop = 1 or gataSpalare = '1') then elapsedTime &lt;= "000000000";
	else elapsedTime &lt;= std_logic_vector(to_unsigned(to_integer(unsigned(time))-count, elapsedTime'length));
	end if;
end process;

END TypeArchitecture;


</vhdl>
  <vhdl name="DecoderTime">LIBRARY ieee;
USE ieee.std_logic_1164.all;
USE ieee.std_logic_unsigned.all;
USE ieee.numeric_std.all;

ENTITY DecoderTime IS
  PORT (
  	inTemp: in std_logic_vector(1 downto 0);
  	enable: in std_logic;
  	mode: in std_logic_vector(2 downto 0);
  	modSelectie: in std_logic;
	prespalare: in std_logic;
	clatireSuplimentara: in std_logic;

  	outTimp: out std_logic_vector(8 downto 0)
    );
END DecoderTime;

--temperatura
--00 - 30
--01 - 40
--10 - 60
--11 - 90

-- decodificator care calculeaza timpul unui ciclu de spalare in functie de temperatura
-- util pt TimerK

ARCHITECTURE TypeArchitecture OF DecoderTime IS

BEGIN

-- Timp spalare: 2 * inTemp - 10 + (2 * inTemp - 20, if prespalare) +10 (+10, if clatireSuplimentara) + 10

-- Timp total normal: 2 * inTemp + 10
-- Timp total prespalare, nu clatire: 4 * inTemp - 10
-- Timp total nu prespalare, clatire: 2 * inTemp +20
-- Timp total prespalare, clatire: 4 * inTemp

--0, manual
--1, automat

process(inTemp, enable, mode, modSelectie, prespalare, clatireSuplimentara)
begin
	if(enable = '1' and modSelectie ='0') then
		case inTemp is
		when "00" =&gt; --temp 30
		if(prespalare = '0' and clatireSuplimentara = '0') then outTimp &lt;="001000110";
		elsif(prespalare = '1' and clatireSuplimentara = '0') then outTimp &lt;="001101110";
		elsif(prespalare = '0' and clatireSuplimentara = '1') then outTimp &lt;="001010000";
		elsif(prespalare = '1' and clatireSuplimentara = '1') then outTimp &lt;="001111000";
		end if;
		
		when "01" =&gt; -- temp 40
		if(prespalare = '0' and clatireSuplimentara = '0') then outTimp &lt;="001011010";
		elsif(prespalare = '1' and clatireSuplimentara = '0') then outTimp &lt;="010010110";
		elsif(prespalare = '0' and clatireSuplimentara = '1') then outTimp &lt;="001100100";
		elsif(prespalare = '1' and clatireSuplimentara = '1') then outTimp &lt;="010100000";
		end if;
		
		when "10" =&gt; -- temp 60
		if(prespalare = '0' and clatireSuplimentara = '0') then outTimp &lt;="010000010";
		elsif(prespalare = '1' and clatireSuplimentara = '0') then outTimp &lt;="011100110";
		elsif(prespalare = '0' and clatireSuplimentara = '1') then outTimp &lt;="010001100";
		elsif(prespalare = '1' and clatireSuplimentara = '1') then outTimp &lt;="011110000";
		end if;
		
		when "11" =&gt; -- temp 90
		if(prespalare = '0' and clatireSuplimentara = '0') then outTimp &lt;="010111110";
		elsif(prespalare = '1' and clatireSuplimentara = '0') then outTimp &lt;="101011110";
		elsif(prespalare = '0' and clatireSuplimentara = '1') then outTimp &lt;="011001000";
		elsif(prespalare = '1' and clatireSuplimentara = '1') then outTimp &lt;="101101000";
		end if;
		
		when others =&gt; outTimp&lt;="000000000";
		end case;
	elsif(enable = '1' and modSelectie = '1') then
		case mode is
		when "000" =&gt; outTimp&lt;="001000110"; -- temp 30
		when "001" =&gt; outTimp&lt;="010000010"; -- temp 60
		when "010" =&gt; outTimp&lt;="001100100"; -- temp 40, clatireSupl
		when "011" =&gt; outTimp&lt;="010010110"; -- temp 40, prespalare
		when "100" =&gt; outTimp&lt;="011001000"; -- temp 90, clatireSupl
		when others =&gt; outTimp&lt;="000000000";
		end case;
	end if;
end process;

END TypeArchitecture;
