  <vhdl name="DecoderTemp">LIBRARY ieee;
USE ieee.std_logic_1164.all;
USE ieee.std_logic_unsigned.all;
USE ieee.numeric_std.all;

ENTITY DecoderTemp IS
  PORT (
  	inTemp: in std_logic_vector(1 downto 0);
  	enable: in std_logic;
  	mode: in std_logic_vector(2 downto 0);
  	modSelectie: in std_logic;
  	prespalare: in std_logic;

  	outTimpSpalare: out std_logic_vector(8 downto 0);
  	outTimpPrespalare: out std_logic_vector(8 downto 0)
    );
END DecoderTemp;

-- Calculates the time for spalare and prespalare using formula spalare and formula prespalare
-- Formula spalare : 2 * inTemp - 10 for spalare
-- Formula prespalare : 2 * inTemp - 20 for prespalare
-- util for TimerK

--temperatura
--00 - 30
--01 - 40
--10 - 60
--11 - 90

--0, mod manual
--1, mod automat

ARCHITECTURE TypeArchitecture of DecoderTemp is

BEGIN

process(inTemp, enable, mode, modSelectie, prespalare)
begin
	if(enable = '1' and modSelectie ='0') then
		case inTemp is
		when "00" =&gt; --temp 30
		outTimpSpalare &lt;="000110010";
		outTimpPrespalare&lt;="111111111"; 
		if(prespalare = '1') then outTimpPrespalare &lt;="000101000"; end if;
		
		when "01" =&gt; -- temp 40
		outTimpSpalare &lt;="001000110";
		outTimpPrespalare&lt;="111111111"; 
		if(prespalare = '1') then outTimpPrespalare &lt;="000111100"; end if; 
		
		when "10" =&gt; -- temp 60
		outTimpSpalare &lt;="001101110";
		outTimpPrespalare&lt;="111111111";
		if(prespalare = '1') then outTimpPrespalare &lt;="001100100"; end if;
		
		when "11" =&gt; -- temp 90
		outTimpSpalare &lt;="010101010";
		outTimpPrespalare&lt;="111111111";
		if(prespalare = '1') then outTimpPrespalare &lt;="010100000"; end if;
		
		when others =&gt; outTimpSpalare&lt;="111111111"; outTimpPrespalare&lt;="111111111";
		end case;
	elsif(enable = '1' and modSelectie = '1') then
		case mode is
		when "000" =&gt; outTimpSpalare &lt;="000110010"; outTimpPrespalare&lt;="111111111";-- temp 30
		when "001" =&gt; outTimpSpalare &lt;="001101110"; outTimpPrespalare&lt;="111111111";-- temp 60
		when "010" =&gt; outTimpSpalare &lt;="001000110"; outTimpPrespalare&lt;="111111111";-- temp 40, clatireSupl
		when "011" =&gt; outTimpSpalare &lt;="001000110"; outTimpPrespalare &lt;="000111100"; -- temp 40, prespalare
		when "100" =&gt; outTimpSpalare&lt;="011001000"; outTimpPrespalare&lt;="111111111";-- temp 90, clatireSupl
		when others =&gt; outTimpSpalare&lt;="111111111"; outTimpPrespalare&lt;="111111111";
		end case;
	end if;
end process;

END TypeArchitecture;</vhdl>
  <vhdl name="BitsToSZU">LIBRARY ieee;
USE ieee.std_logic_1164.all;
USE ieee.std_logic_unsigned.all;
USE ieee.numeric_std.all;

ENTITY BitsToSZU IS
  PORT (
  	input: in std_logic_vector(8 downto 0);
  	sute, zeci, unitati : out std_logic_vector(3 downto 0)
    );
END BitsToSZU;

-- transforma un nr pe 9 biti in unitati, zeci si sute
-- util pt 7 seg

ARCHITECTURE TypeArchitecture OF BitsToSZU IS

BEGIN

process(input)

variable nr, a, b, c: integer:=0;

begin
	nr := to_integer(unsigned(input));
	a := nr/100; b:= (nr/10) mod 10; c:= nr mod 10;

	case a is
		when 0 =&gt; sute &lt;= "0000";
		when 1 =&gt; sute &lt;= "0001";
		when 2 =&gt; sute &lt;= "0010";
		when 3 =&gt; sute &lt;= "0011";
		when 4 =&gt; sute &lt;= "0100";
		when 5 =&gt; sute &lt;= "0101";
		when 6 =&gt; sute &lt;= "0110";
		when 7 =&gt; sute &lt;= "0111";
		when 8 =&gt; sute &lt;= "1000";
		when 9 =&gt; sute &lt;= "1001";
		when others =&gt; sute &lt;= "1111";
	end case;

	case b is
		when 0 =&gt; zeci &lt;= "0000";
		when 1 =&gt; zeci &lt;= "0001";
		when 2 =&gt; zeci &lt;= "0010";
		when 3 =&gt; zeci &lt;= "0011";
		when 4 =&gt; zeci &lt;= "0100";
		when 5 =&gt; zeci &lt;= "0101";
		when 6 =&gt; zeci &lt;= "0110";
		when 7 =&gt; zeci &lt;= "0111";
		when 8 =&gt; zeci &lt;= "1000";
		when 9 =&gt; zeci &lt;= "1001";
		when others =&gt; zeci &lt;= "1111";
	end case;

	case c is
		when 0 =&gt; unitati &lt;= "0000";
		when 1 =&gt; unitati &lt;= "0001";
		when 2 =&gt; unitati &lt;= "0010";
		when 3 =&gt; unitati &lt;= "0011";
		when 4 =&gt; unitati &lt;= "0100";
		when 5 =&gt; unitati &lt;= "0101";
		when 6 =&gt; unitati &lt;= "0110";
		when 7 =&gt; unitati &lt;= "0111";
		when 8 =&gt; unitati &lt;= "1000";
		when 9 =&gt; unitati &lt;= "1001";
		when others =&gt; unitati &lt;= "1111";
	end case;
	
end process;

END TypeArchitecture;
